<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_khach_hang_birthday_campaign" model="ir.cron">
            <field name="name">Thêm KH sinh nhật vào chiến dịch</field>
            <field name="model_id" ref="model_khach_hang"/>
            <field name="state">code</field>
            <field name="code">model._cron_add_birthdays_to_campaign()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="priority">5</field>
        </record>
        
        <record id="ir_cron_chien_dich_update_expired" model="ir.cron">
            <field name="name">Cập nhật chiến dịch hết hạn</field>
            <field name="model_id" ref="model_chien_dich"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_expired_campaigns()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="priority">5</field>
        </record>
    </data>
</odoo>
